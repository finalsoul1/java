package p794;

public class Test3 {
	public static void main(String[] args) {

	}
}

class MyThread extends Thread {
	@Override
	public void run() {
		super.run();
	}
}

class MyRunnable {
	public static void count(String who) throws InterruptedException {
		for (int i = 0; i < 10; i++) {
			System.out.println(who + i + " ");
			Thread.sleep(500);
		}
	}
}